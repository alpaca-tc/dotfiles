eval.c:8484
NORETURN(static void rb_thread_main_jump _((VALUE, int)));
static void
rb_thread_main_jump(err, tag)
    VALUE err;
    int tag;
{
    curr_thread = main_thread;
    th_raise_argc = 1;
    th_raise_argv[0] = err;
    th_raise_node = ruby_current_node;
    rb_thread_restore_context(main_thread, tag);
}
