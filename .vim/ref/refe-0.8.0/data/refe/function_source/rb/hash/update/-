hash.c:943
static VALUE
rb_hash_update(hash1, hash2)
    VALUE hash1, hash2;
{
    hash2 = to_hash(hash2);
    if (rb_block_given_p()) {
	st_foreach(RHASH(hash2)->tbl, rb_hash_update_block_i, hash1);
    }
    else {
	st_foreach(RHASH(hash2)->tbl, rb_hash_update_i, hash1);
    }
    return hash1;
}
