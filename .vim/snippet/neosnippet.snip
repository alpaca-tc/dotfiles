snippet snip
  snippet ${1:name}
  abbr  ${2:abbreviation}
  alias
  regexp
  options
    ${1}

snippet sn
 snippet ${1}
 abbr    $1${2}
 options word
   $1${3}

snippet snr
 snippet ${1}
 abbr    $1${2}
 regexp  '\.[$1]*$'
 options word
   $1${3}

snippet s
  snippet ${1}
    ${2}

snippet ow
  options word

snippet dot
abbr    regexp '\.\w*$' # snip dot
regexp  '\.[[:alnum:]_?]*$'
options word indent

snippet al
  alias   ${1}

snippet alias
abbr    alias
options word
  alias   ${1}
