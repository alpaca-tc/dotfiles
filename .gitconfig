[user]
  email = alprhcp666@gmail.com
  name = Hiroyuki Ishii

[core]
  editor = /Applications/MacVim.app/Contents/MacOS/Vim
  excludesfile = /Users/taichou/.gitignore

[color]
  ui = true

[alias]
  ci = commit
  st = status
  rs = reset
  tr = log --graph --pretty='format:%C(yellow)%h%Creset %C(magenta)%cd%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset' --date=iso
  st = !sh -c 'git ls-remote . | grep `git log -1 --grep="git-svn-id"`
  dm = !git branch --merged | grep -v '*' | xargs -I % git branch -d %
  sn= git shortlog -sn

[push]
  default = tracking

[github]
  user = taichouchou2
  token = a5577564bfa714ebff7d565d7406f08dcd22ac40

[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path =

[mergetool "sourcetree"]
  cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[rerere]
    enabled = 1
[help]
    autocorrect = 1
